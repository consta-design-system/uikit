  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
  "types": "./dist/components/index.d.ts",
  "files": [
    "dist",
    "src/themes/*.css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "node ./scripts/test.js ",
    "start": "start-storybook -p 6006 -s public",
    "build": "NODE_ENV='production' rollup -c",
    "build:storybook": "build-storybook -s public -o build/storybook",
    "tsc-dry-run": "tsc --noEmit",
    "lint:js": "eslint --ext .js,.jsx,.tsx,.ts --fix .",
    "lint:css": "stylelint --fix src/**/*.css",
    "format": "prettier --write '**/*.{json,md}'",
    "svgo": "svgo -f src/icons --config=src/config/svgo.config.json",
    "lint": "concurrently --raw 'yarn run lint:js' 'yarn run lint:css' 'yarn run format'",
    "prepush": "yarn run tsc-dry-run"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "not ie 11"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run prepush"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.css": [
      "stylelint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.svg": [
      "svgo --config=src/config/svgo.config.json",
      "git add"
    ]
  },
  "devDependencies": {
    "@storybook/addon-info": "^5.2.5",
    "awesome-typescript-loader": "^5.2.1",
    "react-docgen-typescript-loader": "^3.3.0",
    "ts-jest": "^24.1.0"
  },
  "homepage": "https://gpn-prototypes.github.io/ui-kit",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/config/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
