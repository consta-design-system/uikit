{
  "name": "@consta/stand",
  "version": "0.0.1",
  "main": "index.ts",
  "keywords": [
    "ui-kit",
    "design-system",
    "consta",
    "gazprom-neft-design-system"
  ],
  "dependencies": {
    "@bem-react/classname": "^1.5.12",
    "@bem-react/classnames": "^1.3.10",
    "@consta/uikit": "^3.24.0",
    "@mdx-js/loader": "^2.1.1",
    "@mdx-js/react": "^2.1.1",
    "compute-scroll-into-view": "^1.0.17",
    "react-transition-group": "^4.4.2"
  },
  "files": [],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "tsc-dry-run": "tsc --noEmit",
    "lint:js": "eslint --ext .js,.jsx,.tsx,.ts --fix . --quiet",
    "lint:css": "stylelint --fix src/**/*.css",
    "lint": "yarn run lint:js && yarn run lint:css",
    "format:svg": "prettier --write '**/*.svg' --parser html",
    "pre-push": "yarn run tsc-dry-run",
    "pre-commit": "yarn run lint-staged",
    "pre-build": "node builder/preBuild.js --config=./build.config.js",
    "test": "yarn lint && yarn tsc-dry-run && yarn unit",
    "unit": "jest",
    "unit:watch": "jest --watch",
    "unit:clear": "jest --clearCache",
    "build:css": "NODE_ENV=production postcss 'src/**/*.css' --base src --dir lib",
    "build:js": "NODE_ENV=production BUILDING_JS=true babel src --out-dir lib --extensions '.ts,.tsx' --source-maps",
    "build:types": "NODE_ENV=production ttsc --declaration --outDir lib/ --emitDeclarationOnly -p tsconfig.build.json",
    "build:package.json": "node scripts/prepare-package-json.js",
    "build:docs": "cp README.md lib/README.md",
    "build": "rm -rf lib && yarn build:css && yarn build:js && yarn build:types && yarn build:package.json && yarn build:docs",
    "stand:start": "NODE_ENV=standDevelopment webpack serve --open --progress --mode development --hot --progress --config webpack.config.js"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "yarn run pre-commit",
      "pre-push": "yarn run pre-push"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.css": [
      "stylelint --fix",
      "git add"
    ],
    "*.{json,md,mdx}": [
      "prettier --write",
      "git add"
    ],
    "*.svg": [
      "prettier --write --parser html",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./commitizen.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "@reatom/core": "^2.0.17",
    "@reatom/react": "^2.0.15",
    "@svgr/core": "^6.2.1",
    "@svgr/plugin-jsx": "^6.2.1",
    "@svgr/plugin-prettier": "^6.1.2",
    "@svgr/plugin-svgo": "^6.2.0",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.7.13",
    "@types/react-transition-group": "^4.4.4",
    "autoprefixer": "^10.4.4",
    "babel-jest": "^27.4.2",
    "babel-loader": "^8.2.4",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.0.1",
    "bfj": "^7.0.2",
    "browserslist": "^4.18.1",
    "camelcase": "^6.2.1",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "commitizen": "^4.2.4",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "cz-customizable": "^6.3.0",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.0.1",
    "github-release-notes": "^0.17.3",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-resolve": "^27.4.2",
    "jest-watch-typeahead": "^1.0.0",
    "mini-css-extract-plugin": "^2.6.0",
    "postcss": "^8.4.12",
    "postcss-color-function": "^4.1.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^6.2.1",
    "postcss-mixins": "^9.0.2",
    "postcss-nested": "^5.0.6",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.0.1",
    "prettier": "^2.6.2",
    "prompts": "^2.4.2",
    "react-app-polyfill": "^3.0.0",
    "react-dev-utils": "^12.0.0",
    "react-dom": "^18.0.0",
    "react-refresh": "^0.11.0",
    "react-router5": "^8.0.1",
    "react-test-renderer": "^18.0.0",
    "resolve": "^1.20.0",
    "resolve-url-loader": "^4.0.0",
    "router5": "^8.0.1",
    "router5-helpers": "^8.0.0",
    "router5-plugin-browser": "^8.0.1",
    "sass-loader": "^12.3.0",
    "semver": "^7.3.5",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.6.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "tailwindcss": "^3.0.2",
    "terser-webpack-plugin": "^5.2.5",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "typescript-eslint-parser": "^22.0.0",
    "url-loader": "4.1.1",
    "use-subscription": "^1.5.1",
    "web-vitals": "^2.1.0",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1",
    "webpack-manifest-plugin": "^4.0.2",
    "workbox-webpack-plugin": "^6.4.1"
  },
  "homepage": "https://github.com/consta-design-system/stand",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
